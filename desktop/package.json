{
  "name": "@seekr/desktop",
  "private": true,
  "version": "1.0.0",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4102",
    "build": "vite build && electron-builder",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "electron:dev": "npx kill-port 4102 && vite --port 4102"
  },
  "dependencies": {
    "@subbiah/reusable": "github:subbiah2806/component",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@types/node": "24.9.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "4.3.0",
    "autoprefixer": "10.4.19",
    "concurrently": "9.1.0",
    "electron": "33.2.0",
    "electron-builder": "25.1.8",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-electron": "0.28.8",
    "wait-on": "8.0.1"
  },
  "build": {
    "appId": "com.seekr.desktop",
    "productName": "Seekr",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    }
  }
}
