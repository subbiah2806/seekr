{
  "name": "@seekr/extension",
  "version": "1.0.0",
  "description": "Chrome extension for AI-powered resume tailoring",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "preview": "vite preview",
    "reset": "rm -rf ./nodemodules && rm -rf package-lock.json",
    "install-link": "npm run reset && npm i && cd ../../component && rm -rf ./nodemodules && rm -rf package-lock.json && npm i && npm link && cd . && npm link @subbiah/reusable"
  },
  "dependencies": {
    "@subbiah/reusable": "github:subbiah2806/component",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.2.1",
    "@types/chrome": "0.0.254",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "5.9.3",
    "vite": "5.0.8"
  }
}
